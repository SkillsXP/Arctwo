//actual main code
package cornellNotes;
import javax.swing.*;
public class actualMain {
	public static void main(String args[]){
		JFrame j = new JFrame("Cornell Notes");
		j.setVisible(true);
		j.setSize(500, 500);
		j.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		drawingNotes dn = new drawingNotes();
		j.add(dn);
		dn.paint();
	}
}
